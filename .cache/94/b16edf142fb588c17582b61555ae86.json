\r\n  if (data.length > 10) {\r\n    bigError();\r\n  }\r\n}\r\n\r\nfunction clearInput() {\r\n  refs.inputSearchEl.value = '';\r\n}\r\n\r\nfunction clearCardCont() {\r\n  refs.cardCont.innerHTML = '';\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"45a683f0e3a1a14964b03f97328bcf3f","cacheData":{"env":{}}}